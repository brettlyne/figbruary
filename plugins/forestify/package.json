{
  "name": "forestify",
  "version": "1.0.0",
  "description": "Figma plugin to generate a forest given some set of trees",
  "type": "module",
  "scripts": {
    "build:code": "tsc",
    "build:ui": "npx vite build",
    "build:code:watch": "tsc --watch",
    "build:ui:watch": "npx vite build --watch",
    "build:ui:dev:watch": "npx vite build --watch --mode development",
    "build:watch": "npm run build:ui:watch & npm run build:code:watch",
    "build": "npm run build:ui && npm run build:code",
    "dev:ui": "npx vite"
  },
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "@figma/plugin-typings": "^1.85.0",
    "@mui/material": "^5.15.7",
    "@types/react": "^18.2.51",
    "@types/react-dom": "^18.2.18",
    "@vitejs/plugin-react": "^4.2.1",
    "prettier": "^3.2.4",
    "typescript": "^5.3.3",
    "vite": "^5.0.12",
    "vite-plugin-singlefile": "^1.0.0"
  },
  "dependencies": {
    "@emotion/react": "^11.11.3",
    "@emotion/styled": "^11.11.0",
    "@mui/icons-material": "^5.15.7",
    "@mui/material-next": "6.0.0-alpha.120",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "sass": "^1.70.0"
  }
}
